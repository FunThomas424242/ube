<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="ube_start_engine">


	<target name="start_engine" description="Start the Equinox Engine">

		<echo>Starting Equinox Engine</echo>
		<ube Shutdown="false">
			<condition.equals arg1="true" arg2="true" />
			<condition.equals arg1="blah" arg2="blah" />
		</ube>

	</target>



	<target name="stop_engine" description="Stop the Equinox Engine">

		<echo>Stopping Equinox Engine</echo>
		<ube Shutdown="true">
			<condition.equals arg1="true" arg2="true" />
			<condition.equals arg1="blah" arg2="blah" />
		</ube>

	</target>



</project>